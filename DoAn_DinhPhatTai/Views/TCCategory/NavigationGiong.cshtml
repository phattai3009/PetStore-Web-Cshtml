@model List <Model.EF.Giong>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Navigation Giống</title>
</head>
<body>
    <div class="sidebar_section">
        <div class="sidebar_title">
            <h5>Thú Cưng Theo Giống</h5>
        </div>
        <ul class="sidebar_categories">
            @foreach (var item in Model)
            {
                <li><a href="@Url.Action("ShowThuCungTheoGiong", new {@MaGiong=item.@MaGiong })"><span style="margin-right:10px;"></span><b>@item.TenGiong</b></a></li>
            }
            <li><a href=@Url.Action("AllGiongThuCungCategory")><span style="margin-right:10px;"></span><b>Xem thêm ...</b></a></li>
        </ul>
    </div>
</body>
</html>
