@model List<Model.EF.Loai>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Navigation Loài</title>
</head>
<body>
    <div class="sidebar_section">
        <div class="sidebar_title">
            <h5>Thú Cưng Theo Loài</h5>
        </div>
        <ul class="sidebar_categories">
            @foreach (var item in Model)
            {
                <li><a href="@Url.Action("ShowThuCungTheoLoai", new {@MaLoai=item.MaLoai })"><span style="margin-right:10px;"></span><b>@item.TenLoai</b></a></li>
            }
        </ul>
    </div>
</body>
</html>
